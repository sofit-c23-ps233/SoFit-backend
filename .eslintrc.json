{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console"    : "off",
        "global-require": "off",
        "func-names"    : "off",
        "consistent-return": "off",
        "no-shadow"     : "off"
    }
}
